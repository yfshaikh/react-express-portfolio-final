{
  "version": 2,
  "builds": [
    {
      "src": "./server/server.js",  
      "use": "@vercel/node"
    },
    {
      "src": "dist/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
