{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "https://react-express-portfolio-final-frontend.vercel.app/(.*)",
      "dest": "../src/App.jsx"
    }
  ]
}