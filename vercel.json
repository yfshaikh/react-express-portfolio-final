{
  "version": 2,
  "builds": [
    {
      "src": "./server/server.js",  
      "use": "@vercel/node"
    },
    {
      "src": "./dist",    
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/server/server.js"
    },
    {
      "source": "/api/(.*)",
      "destination": "/src/index.jsx"
    }
  ]
}
